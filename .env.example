# Discord Multi-Agent System Environment Variables
# TDD Phase 2 - テスト環境用設定例

# Environment Mode
ENV_MODE=test  # test/development/production

# Discord Bot Tokens (統合受信・個別送信型アーキテクチャ)
RECEPTION_BOT_TOKEN=your_reception_bot_token_here
SPECTRA_BOT_TOKEN=your_spectra_bot_token_here
LYNQ_BOT_TOKEN=your_lynq_bot_token_here
PAZ_BOT_TOKEN=your_paz_bot_token_here

# Discord Configuration
TARGET_GUILD_ID=your_discord_server_id_here
COMMAND_CENTER_CHANNEL_ID=your_command_center_channel_id
CREATION_CHANNEL_ID=your_creation_channel_id
DEVELOPMENT_CHANNEL_ID=your_development_channel_id
LOUNGE_CHANNEL_ID=your_lounge_channel_id

# AI Service Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_TEMPERATURE=0.4
MAX_OUTPUT_TOKENS=1000

# Database Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=discord_agent_test
POSTGRES_USER=test_user
POSTGRES_PASSWORD=test_password

# Test Configuration
DEBUG_MODE=true
LOG_LEVEL=DEBUG